import chembook
from chembook import ChemBookSearch


iter=0
with open('DTSC_CAS.csv') as myfile:
    for cas in myfile:
        if cas is not None:
            iter=iter+1
            test1=ChemBookSearch()
            test1.findChemical(cas)
            print "Iter at ", iter,'File saved'
        else:
            print "Empty CAS!"




