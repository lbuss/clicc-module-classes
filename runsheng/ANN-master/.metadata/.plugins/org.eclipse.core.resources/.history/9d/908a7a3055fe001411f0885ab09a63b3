if __name__ == '__main__':
    class_count=dict()
    fhand=open('./data/DTSC_classification_chembook.csv')
    for line in fhand:
        line=line.rstrip()
        line=line.split(',')
        #strip CAS number:
        if line[1]is '':
            class_count['No class']=class_count.get('No class',0)+1
            
        
        else: 
            for name in line[1:]:
                if name is not '':
                    '''deal with dict here'''
    
                    class_count[name]=class_count.get(name,0)+1
        
       
    for chem_class,count in class_count.items():
